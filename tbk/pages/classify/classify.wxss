/* pages/classify/classify.wxss */

.ul {
  display: flex;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
  -ms-flex-direction: row;
  flex-direction: row;
  -ms-flex-align: center;
  align-items: center;
  padding-bottom: 133rpx;
  border-bottom: 2rpx solid #f1f1f1;
  height: 205rpx;
}

.li {
  display: inline-block;
  width: 25%;
  box-sizing: border-box;
  text-align: center;
  margin-top: 15rpx;
}

.menImg {
  display: block;
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto;
  background: #f1f1f1;
}

.menTxt {
  display: inline-block;
  /* width: 25%; */
  box-sizing: border-box;
  text-align: center;
  margin-top: 3rpx;
  font-size:26rpx;

}

.sortBox {
  display: flex;
  height: 45rpx;
  line-height: 45rpx;
  border-bottom: 1px solid #f1f1f1;
  background: #fff;
}

.sortTxt {
  flex: 1;
  text-align: center;
  font-size: 26rpx;
  cursor: pointer;
  width:30%;

}
.sortImg{

  width:34rpx;
height:34rpx;
display:inline-block;
padding-top:-1rpx;
margin-:0.5px;
line-height:39rpx;
margin-bottom:-7rpx;

}

.listli {
  display: inline-block;
  width: 50%;
  box-sizing: border-box;
  border-right: 1px solid #f1f1f1;
  border-bottom: 1px solid #f1f1f1;
  padding: 0.4rem 0;
  cursor: pointer;
}

.goods-title {
  overflow: hidden;
  text-overflow: ellipsis;
  float: left;
  margin-left: 19px;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  margin: 1rpx 0 1rpx 1rpx;
  height: 83rpx;
  font-size: 20rpx;
  background: #fff;
  vertical-align: middle;
  color: #666;
  width: 280rpx;
  margin-top: -25rpx;
}

.listImgs {
  display: block;
  width: 8.3rem;
  height: 5.6rem;
  background: #f1f1f1;
  margin: 0 auto;
}

.listdesc {
  font-size: 20rpx;
  vertical-align: middle;
  color: #666;
  background: #fff;
  margin-left: 15rpx;
  height: 33px;
}

.coupon {
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 50% 100%;
  padding-left: 100rpx;
  color: #fff;
  font-size: 40rpx;
  height: 120rpx;
  line-height: 56rpx;
  margin: 10rpx 0rpx;
  margin-left: 15rpx;
}
